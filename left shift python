print("Enter the digit")
a=int(input())
print("left shift = ",a<<1)
print("left shift = ",a<<2)
print("left shift = ",a<<3)
print("left shift = ",a<<4)
print("left shift = ",a<<5)
print("left shift = ",a<<6)
print("left shift = ",a<<7)
print("left shift = ",a<<8)
print("left shift = ",a<<9)
print("left shift = ",a<<10)
