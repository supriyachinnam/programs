#include <stdio.h>
#include <string.h>
void main()
{
    char str[50];
    int n,x,i,k,j;
    printf("Enter a string:");
    gets(str);
    n=strlen(str);
    for(i=1;i<=n;i++)
    {
        for(j=0;j<=n-i;j++)
        {
            int x=j+i;
            for(k=j;k<x;k++)
            printf("%c",str[k]);
            printf("\n");
        }
    }
}
