int main()
{
    int arr[5],i,n,total,sum=0,diff;
    printf("Enter:");
    scanf("%d",&n);
    total=n*(n+1)/2;
    printf("%d",total);
    printf("Enter elements");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        sum=sum+arr[i];
        printf("%d",sum);
    }
    diff=total-sum;
    printf("%d",diff);
    return 0;
}
