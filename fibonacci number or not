#include <stdio.h>
#include<conio.h>

int main()
    {
        int t1=0,t2=1,t3,n;
        printf("Enter input:");
        scanf("%d",&n);
        t3=t1+t2;
        while(t3<n)
        {
            t1=t2;
            t2=t3;
            t3=t1+t2;
        }
        if(t3==n)
        printf("YES");
        else
        printf("NO");
        return 0;
    }
